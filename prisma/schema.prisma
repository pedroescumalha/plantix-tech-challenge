generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model SensorData {
  id         Int    @id @default(autoincrement())
  externalId String @map("external_id")
  type       String
  value      String
  valueType  String @map("value_type")
  unit       String
  latitude   Float
  longitude  Float
  timestamp  BigInt
  createdAt  BigInt @default(dbgenerated("(strftime('%s', 'now'))")) @map("created_at")
  updatedAt  BigInt @default(dbgenerated("(strftime('%s', 'now'))")) @map("updated_at")

  @@map("sensor_data")
}
