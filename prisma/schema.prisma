// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:memdb1?mode=memory&cache=shared"
}

model SensorData {
  id         Int    @id @default(autoincrement())
  externalId String @map("external_id")
  type       String
  value      String
  valueType  String @map("value_type")
  unit       String
  latitude   Float
  longitude  Float
  timestamp  BigInt
  createdAt  BigInt @default(dbgenerated("extract(epoch from now())")) @map("created_at")
  updatedAt  BigInt @default(dbgenerated("extract(epoch from now())")) @map("updated_at")
}
